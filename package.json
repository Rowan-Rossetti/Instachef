{
  "name": "instachef",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build && ng run instachef:server",
    "serve:ssr": "node dist/instachef/server/main.js",
    "dev:ssr": "ng run instachef:serve-ssr",
    "test": "ng test",
    "deploy": "ng build --configuration production --base-href \"https://Rowan-Rossetti.github.io/Instachef/\" && npx angular-cli-ghpages --dir=dist/instachef/browser --branch=gh-pages",
    "deploy:gh": "ng deploy --base-href=/Instachef/"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.11",
    "@angular-eslint/builder": "^19.7.1",
    "@angular-eslint/eslint-plugin": "^19.7.1",
    "@angular-eslint/eslint-plugin-template": "^19.7.1",
    "@angular-eslint/template-parser": "^19.7.1",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.10",
    "@types/angular": "^1.8.9",
    "@types/express": "^5.0.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.15.17",
    "angular-cli-ghpages": "^2.0.3",
    "sass": "^1.89.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.19",
    "@angular/forms": "^19.2.14",
    "@angular/localize": "^19.2.10",
    "@angular/material": "^19.2.19",
    "@angular/platform-server": "^19.2.10",
    "@angular/router": "^19.2.10",
    "@angular/ssr": "^19.2.11",
    "domutils": "^3.2.2",
    "express": "^4.18.2",
    "html2pdf.js": "^0.10.3",
    "is-number": "^7.0.0"
  }
}
